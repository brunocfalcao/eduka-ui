{
    "name": "brunocfalcao/eduka-ui",
    "description": "Eduka UI library",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/brunocfalcao/eduka-ui",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "autoload": {
        "psr-4": {
            "Eduka\\UI\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eduka\\UI\\UIServiceProvider"
            ],
            "aliases": {
                "UI": "Eduka\\UI\\Facades\\UI"
            }
        }
    }
}
